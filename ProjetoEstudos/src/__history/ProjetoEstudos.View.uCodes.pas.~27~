unit ProjetoEstudos.View.uCodes;

interface

implementation
uses
ProjetoEstudos.View.Cadastro, ProjetoEstudos.View.Main, System.SysUtils;

Procedure TelaCadastroClick();
begin
  FormCadastro := FormCadastro.Create( Self );

  FormCadastro.Parent := FormPrincipal.PnlPrincipal;
  FormCadastro.Align := alClient;
  FormCadastro.BorderStyle := bsSingle;

  FormCadastro.Show;

end;

end.
